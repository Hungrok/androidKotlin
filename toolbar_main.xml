<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <ImageView
            android:layout_width="103dp"
            android:layout_height="139dp" tools:srcCompat="@drawable/ic_home_black_24dp"
            android:id="@+id/imageView1" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="148dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="30dp" app:layout_constraintEnd_toStartOf="@+id/imageView2"
            android:layout_marginEnd="70dp"/>
    <ImageView
            android:layout_width="126dp"
            android:layout_height="128dp" tools:srcCompat="@drawable/ic_insert_emoticon_black_24dp"
            android:id="@+id/imageView2" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="158dp" android:layout_marginStart="70dp"
            app:layout_constraintStart_toEndOf="@+id/imageView1"
            android:layout_marginBottom="445dp"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <Switch
            android:text="Switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/switchButton"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="498dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="152dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="177dp"
            android:layout_marginBottom="206dp" app:layout_constraintBottom_toBottomOf="parent"/>
</android.support.constraint.ConstraintLayout>