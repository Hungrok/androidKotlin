    <TextView
            android:text="파"
            android:layout_width="43dp"
            android:layout_height="342dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:id="@+id/fa" app:layout_constraintVertical_bias="0.612" app:layout_constraintStart_toEndOf="@+id/mi"
            android:layout_marginStart="8dp"/>
    <TextView
            android:text="솔"
            android:layout_width="46dp"
            android:layout_height="130dp"
            android:id="@+id/sol" app:layout_constraintStart_toEndOf="@+id/fa" android:layout_marginStart="8dp"
