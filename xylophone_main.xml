            app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
            android:text="라"
            android:layout_width="70dp"
            android:layout_height="236dp"
            android:id="@+id/la" app:layout_constraintStart_toEndOf="@+id/sol" android:layout_marginStart="8dp"
