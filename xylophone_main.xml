            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
            android:text="레"
            android:layout_width="56dp"
            android:layout_height="320dp"
            android:id="@+id/re" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toEndOf="@+id/do1"
            android:layout_marginStart="8dp"/>
    <TextView
            android:text="미"
            android:layout_width="56dp"
            android:layout_height="242dp"
            android:id="@+id/mi" app:layout_constraintStart_toEndOf="@+id/re" android:layout_marginStart="8dp"
