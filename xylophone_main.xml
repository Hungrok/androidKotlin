            app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
            android:text="시"
            android:layout_width="49dp"
            android:layout_height="180dp"
            android:id="@+id/si" app:layout_constraintStart_toEndOf="@+id/la" android:layout_marginStart="8dp"
